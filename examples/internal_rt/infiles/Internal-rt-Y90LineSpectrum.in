#------------------------
#  Source 1
#########################

sources/generic/source1/nhist 4.0e6 
sources/generic/source1/kpar "electron"

# Directional sampling
#########################

sources/generic/source1/direction/type "SOLID_ANGLE"

sources/generic/source1/direction/direction/u 0.0
sources/generic/source1/direction/direction/v 0.0
sources/generic/source1/direction/direction/w 1.0

# Set theta
sources/generic/source1/direction/polar-oberture/theta0 0.0
sources/generic/source1/direction/polar-oberture/theta1 180.0

# Set phi
sources/generic/source1/direction/azimutal-oberture/phi0 0.0
sources/generic/source1/direction/azimutal-oberture/dphi 360.0

# Energy sampling
#########################

sources/generic/source1/energy/type "INTERVALS"


# Set intervals
sources/generic/source1/energy/nintervals 20


# Set energies
sources/generic/source1/energy/lowE [0.0571e6,0.1713e6,0.2855e6,0.3997e6,0.5139e6,0.6281e6,0.7423e6,0.8565e6,0.9707e6,1.0849e6,1.1991e6,1.3133e6,1.4275e6,1.5417e6,1.6559e6,1.7701e6,1.8843e6,1.9985e6,2.1127e6,2.2269e6]
sources/generic/source1/energy/topE [0.0571e6,0.1713e6,0.2855e6,0.3997e6,0.5139e6,0.6281e6,0.7423e6,0.8565e6,0.9707e6,1.0849e6,1.1991e6,1.3133e6,1.4275e6,1.5417e6,1.6559e6,1.7701e6,1.8843e6,1.9985e6,2.1127e6,2.2269e6]

# Set probabilities
sources/generic/source1/energy/probabilities [4.26e-2,5.18e-2,5.94e-2,6.49e-2,6.86e-2,7.08e-2,7.17e-2,7.15e-2,7.04e-2,6.85e-2,6.57e-2,6.19e-2,5.69e-2,5.07e-2,4.30e-2,3.42e-2,2.46e-2,1.50e-2,6.43e-3,1.13e-3]

# Spatial sampling
#########################

# Set image size
sources/generic/source1/spatial/type "IMAGE"
sources/generic/source1/spatial/directory "data/Dicom/SPECT"

# Set image center 
#sources/generic/source1/spatial/position/x  24.918781 
#sources/generic/source1/spatial/position/y  10.793281
#sources/generic/source1/spatial/position/z  37.955624

sources/generic/source1/spatial/isocenter/x 0.0
sources/generic/source1/spatial/isocenter/y 0.0
sources/generic/source1/spatial/isocenter/z 0.0

sources/generic/source1/spatial/phantom-isocenter/x 0.0
sources/generic/source1/spatial/phantom-isocenter/y 0.0
sources/generic/source1/spatial/phantom-isocenter/z 0.0

sources/generic/source1/spatial/ActivityDistribution/read "data/WalkerAliasing/ActivityInitialization.dat"
sources/generic/source1/spatial/ActivityDistribution/save "output/ActivityInitialization-check.dat"

#sources/generic/source1/spatial/ActivityDistribution/save "data/WalkerAliasing/ActivityInitialization.dat"

